{
  "name": "@magiqan/monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/cli",
    "packages/config",
    "packages/constants",
    "packages/core",
    "packages/errors",
    "packages/events",
    "packages/logger",
    "packages/profiler",
    "packages/reporter",
    "packages/runner",
    "packages/runtime",
    "packages/types",
    "packages/tests"
  ],
  "scripts": {
    "start": "node -e 'console.log(`Hello`)'",
    "example": "node --no-warnings --experimental-abortcontroller -r ts-node/register examples/src/index.ts",
