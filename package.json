    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "commitlint": "13.2.1",
    "eslint": "8.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.0",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.3.4"
